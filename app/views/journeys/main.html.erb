
<h1>Ember</h1>
<div class="container main-background" style="background-color: none; ">
  <div class="position">
    <div class="row row-main">
      <li class="action main-button round">
        <%= link_to "Create New Journey", new_journey_path %>
      </li>
    </div>
    <br>
    <br>
    <div class="row row-main">
      <div class="choose_action">
        <a class="btn main-button" data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample" style="text-align: center;">
        Continue Journey
        </a>
        <div class="collapse" id="collapseExample">
          <div class="icon-background">
            <p id="new_picture" class="main-icons inline"><%= link_to new_journey_picture_path(@current_journey) do %><i class="fas fa-camera" aria-hidden="true"></i><% end %></p>
            <p class="main-icons inline"><%= link_to new_journey_audio_path(@current_journey) do %><i class="fas fa-microphone" aria-hidden="true"></i><% end %></p>
            <p class="main-icons inline"><%= link_to new_journey_note_path(@current_journey) do %><i class="fas fa-pencil-alt" aria-hidden="true"></i><% end %></p>
          </div>
        </div>
      </div>
    </div>
    <br>
    <br>
  <div class="row row-main">
    <li class="action main-button round">
      <%= link_to "Journey Albums", journeys_path %>
    </li>
  </div>
  
  </div>
</div>
